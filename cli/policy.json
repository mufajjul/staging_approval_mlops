{
    "mode": "All",
    "policyRule": {
        "if": {
            "anyOf": [{

                    "field": "type",
                    "equals": "Microsoft.MachineLearningServices/workspaces/models/versions/tags"
                },
                {
                    "not": {
                        "allOf": [{
                                "field": "tags['stage']",
                                "Equals": "[parameters('StageName1')]"
                            },
                            {
                                "field": "tags['stage']",
                                "Equals": "[parameters('StageName2')]"
                            },
                            {
                                "field": "tags['stage']",
                                "Equals": "[parameters('StageName3')]"
                            },
                            {
                                "field": "tags['stage']",
                                "Equals": "[parameters('StageName4')]"
                            }
                        ]
                    }
                }
            ]

        },
        "then": {
            "effect": "deny"
        }
    },
    "parameters": {
        "StageName1": {
            "type": "String",
            "metadata": {
                "displayName": "Stage Name 1",
                "description": "Name of the stage, such as 'environment'"
            }
        },
        "StageName2": {
            "type": "String",
            "metadata": {
                "displayName": "Stage Name 2",
                "description": "Name of the stage, such as 'environment'"
            }
        },
        "StageName3": {
            "type": "String",
            "metadata": {
                "displayName": "Stage Name 3",
                "description": "Name of the stage, such as 'environment'"
            }
        },
        "StageName4": {
            "type": "String",
            "metadata": {
                "displayName": "Stage Name 4",
                "description": "Name of the stage, such as 'environment'"
            }
        }
    }
}